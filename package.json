{
  "name": "@elm-node/core",
  "version": "2.0.1",
  "description": "Native bindings to Node.js's core libraries.",
  "scripts": {
    "postinstall": "elm-npm-postinstall",
    "preuninstall": "elm-npm-preuninstall",
    "test": "npm-run-all test:*",
    "test:Buffer": "elm-make test/Buffer/Main.elm --output test/Buffer/main.js && node test/Buffer/index.js",
    "test:Crypto": "elm-make test/Crypto/Main.elm --output test/Crypto/main.js && node test/Crypto/index.js",
    "test:FileSystem": "elm-make test/FileSystem/Main.elm --output test/FileSystem/main.js && node test/FileSystem/index.js",
    "test:Global": "elm-make test/Global/Main.elm --output test/Global/main.js && node test/Global/index.js"
  },
  "repository": "elm-node/core",
  "author": "Alexandre Gigliotti <alex@panosoft.com>",
  "license": "Unlicense",
  "dependencies": {
    "@elm-node/npm-scripts": "^1.0.0",
    "cpr": "^2.0.2",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.1"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-elm": "^2.0.0",
    "npm-run-all": "^4.0.2"
  }
}
